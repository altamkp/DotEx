{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": ["**/*.csproj"]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["**/*.{md,yml}"],
        "exclude": ["_site/**"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "template": ["default", "modern"],
    "globalMetadata": {
      "_appFaviconPath": "images/altamkp.png",
      "_appFooter": "Copyright (c) altamkp",
      "_appLogoPath": null,
      "_appName": "DotnetEx",
      "_appTitle": "DotnetEx",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/altamkp/DotnetEx",
        "branch": "master"
      },
      "pdf": true
    }
  }
}
